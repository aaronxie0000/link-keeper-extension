{
    "name": "Learn Chrome Ext",
    "version": "0.0.1",
    "manifest_version": 3,
    "background":{
        "service_worker": "background.js"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }],
    "permissions": ["storage", "tabs"],
    "action": {
        "default_popup": "popup.html",
        "default_name": "Learning Time"
    }
}